{
  "branches": ["master"],
  "plugins": [
    "@semantic-release/commit-analyzer",
    ["@semantic-release/exec", {
      "prepareCmd": "./prepare.sh ${nextRelease.version}"
    }],
    ["@semantic-release/github", {
      "assets": [
        {"path": "release.zip", "label": "Addon package"}
      ]
    }]
  ]
}
